public class Visitor {
    private String deviceType;
    private String ip;
    private String browser;

    public String getDeviceType() {
        return deviceType;
    }

    public void setDeviceType(String deviceType) {
        this.deviceType = deviceType;
    }

    public String getIp() {
        return ip;
    }

    public void setIp(String ip) {
        this.ip = ip;
    }

    public String getBrowser() {
        return browser;
    }

    public void setBrowser(String browser) {
        this.browser = browser;
    }
}

public class Main {
    public static void main(String[] args) {
        Visitor visitor = new Visitor();
        visitor.setDeviceType("Desktop");
        visitor.setIp("192.168.1.100");
        visitor.setBrowser("Chrome");

        System.out.println("Visitor Details:");
        System.out.println("Device Type: " + visitor.getDeviceType());
        System.out.println("IP: " + visitor.getIp());
        System.out.println("Browser: " + visitor.getBrowser());
    }
}
